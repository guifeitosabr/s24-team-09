{
  "manifest_version": 3,
  "name": "FocusTabs",
  "version": "1.0",
  "description": "Chrome Extension that allows groupings of tabs",
  "icons": {
    "16": "images/favicon-16x16.png",
    "32": "images/favicon-32x32.png",
    "180": "images/apple-touch-icon.png",
    "192": "images/FocusTabsLogo.png",
    "512": "images/FocusTabsLogo.png"
  },
  "offline_enabled": true,
  "permissions": ["storage", "activeTab", "contextMenus", "tabs"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html"
  }
}
